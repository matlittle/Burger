<div class="container">
	<div class="row">
		<div class="col-xs-6">
			<ul>
			{{#each notEaten}}
				<li><p>
					{{this.id}}. {{this.burger_name}} 
						<button data-id={{this.id}} class="delBtn">Delete</button>
				</p></li>
			{{/each}}
			</ul>
		</div>

		<div class="col-xs-6">
			<ul>
			{{#each eaten}}
				<li><p>{{this.id}}. {{this.burger_name}}</p></li>
			{{/each}}
			</ul>
		</div>
	</div>
</div>

